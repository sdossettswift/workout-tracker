#workout
  %p= @workout.date
  %p= @workout.workout
  %p= @workout.length
  %p= @workout.mood
  %p= @workout.comments

#exercises
  %h2 Exercises
  = render @workout.exercises
  %h3 Add an Exercise
  = render "exercises/form"

  =link_to "Back", root_path
  =link_to "Edit", edit_workout_path(@workout)
  =link_to "Delete", workout_path(@workout), method: :delete, data:{confirm: "Are you sure you want to delete this workout?"}
